{
    "name": "zipzoft/laravel-repository",
    "type": "library",
    "license": "MIT",
    "description": "Laravel Repository",
    "keywords": [],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zipzoft\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zipzoft\\Repository\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "PA",
            "role": "DevOps"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
